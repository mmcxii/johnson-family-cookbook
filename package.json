{
  "name": "johnson-family-cookbook",
  "version": "1.0.0",
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "main": "dist/server/proxy/index.js",
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "build:browser": "cross-env NODE_ENV=production webpack --config=\"webpack.config.prod.js\"",
    "build:server": "tsc --project tsconfig.server.json",
    "clean": "rimraf dist",
    "commit-lint": "npx --no-install commitlint --config ./.commitlintrc.json --edit",
    "docker:start:dev": "docker-compose -f ./tools/docker/docker-compose.dev.yml up",
    "docker:start:dev:detach": "npm run docker:start:dev -- --detach",
    "husky:commit-msg": "npm run commit-lint",
    "husky:pre-commit": "npm run lint-staged",
    "husky:pre-push": "npm run test",
    "lint-staged": "npx --no-install lint-staged --concurrent false --config ./.lintstagedrc.json",
    "lint-staged:lint": "eslint --fix",
    "lint-staged:package-json": "sort-package-json",
    "lint-staged:prettier": "prettier --write",
    "lint-staged:sortier": "sortier",
    "lint-staged:stylelint": "stylelint --config ./.stylelintrc.json",
    "lint-staged:test": "jest --findRelatedTests",
    "prepare": "npm run clean && npm run build",
    "start": "npm run docker:start:dev:detach && npm run clean && npm run build:server && concurrently \"npm:watch:server\" \"npm:start:server\" \"npm:watch:browser\"",
    "start:server": "concurrently \"npm:start:server:*\"",
    "start:server:api": "nodemon -r dotenv/config ./dist/server/api/index.js",
    "start:server:proxy": "nodemon -r dotenv/config ./dist/server/proxy/index.js",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "watch:browser": "webpack serve --config=\"webpack.config.dev.js\"",
    "watch:server": "npm run build:server -- --incremental --watch"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@faker-js/faker": "7.2.0",
    "@mikro-orm/cli": "5.2.0",
    "@mikro-orm/core": "5.2.0",
    "@mikro-orm/nestjs": "5.0.2",
    "@mikro-orm/postgresql": "5.2.0",
    "@nestjs/common": "8.4.7",
    "@nestjs/config": "2.1.0",
    "@nestjs/core": "8.4.7",
    "@nestjs/jwt": "8.0.1",
    "@nestjs/passport": "8.2.2",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/testing": "8.4.7",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@types/bcrypt": "5.0.0",
    "@types/eslint": "8.4.3",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.1",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "autoprefixer": "10.4.7",
    "bcrypt": "5.0.1",
    "classnames": "2.3.1",
    "commitlint": "17.0.2",
    "concurrently": "7.2.2",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "cssnano": "5.1.11",
    "dotenv": "16.0.1",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-unicorn": "42.0.0",
    "express": "4.18.1",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "i18next": "21.8.10",
    "i18next-browser-languagedetector": "6.1.4",
    "i18next-http-backend": "1.4.1",
    "informed": "4.18.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "lint-staged": "13.0.2",
    "mini-css-extract-plugin": "2.6.1",
    "nestjs-redis": "1.2.2",
    "nodemon": "2.0.16",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pino-http": "8.1.0",
    "postcss": "8.4.14",
    "postcss-loader": "7.0.0",
    "postcss-nesting": "10.1.8",
    "postgres": "3.2.4",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "11.17.2",
    "react-query": "3.39.1",
    "react-router-dom": "6.3.0",
    "reflect-metadata": "0.1.13",
    "sort-package-json": "1.57.0",
    "sortier": "1.0.1",
    "source-map-loader": "4.0.0",
    "style-loader": "3.3.1",
    "stylelint": "14.9.1",
    "stylelint-config-recommended": "8.0.0",
    "ts-jest": "28.0.5",
    "ts-loader": "9.3.0",
    "typescript": "4.7.3",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.2"
  },
  "mikro-orm": {
    "useTsNode": true,
    "tsConfigPath": "./tsconfig.json",
    "configPaths": [
      "./src/server/api/modules/orm/v1/utils/config.ts"
    ]
  }
}
